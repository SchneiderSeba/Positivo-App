{
  "expo": {
    "name": "try-app",
    "slug": "try-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo-Transp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "tryapp",
    "splash": {
      "image": "./assets/Logo-Transp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo-Transp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.schneiderseba.tryapp",
      "permissions": ["NOTIFICATIONS"],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications"
    ],
    "extra": {
      "router": {},
      "ADMIN_EMAIL": "admin@ejemplo.com",
      "ADMIN_PASSWORD": "123456",    
      "eas": {
        "projectId": "07251189-9eeb-4064-bc1a-386d022fad61"
      }
    }
  }
}
